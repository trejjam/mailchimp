{
	"name": "trejjam/mail-chimp",
	"description": "MailChimp integration into Nette framework",
	"keywords": [
		"nette",
		"mailChimp"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal",
			"homepage": "https://github.com/trejjam"
		}
	],
	"require": {
		"php": "^7.3 | ^8.0",

		"composer/ca-bundle": "^1.1",
		"nette/di": "^3.0",
		"guzzlehttp/guzzle": "^7.0",
		"trejjam/base-extension": "^0.8",
		"tharos/schematic": "^1.2",
		"thecodingmachine/safe": "^1.0"
	},
	"require-dev": {
		"nette/robot-loader": "^3.1",
		"nette/bootstrap": "^2.4.6 || ^3.0",
		"tracy/tracy": "^2.4 || ^3.0",

		"nette/tester": "^2.2",

		"phpstan/phpstan": "^0.12",
		"phpstan/phpstan-deprecation-rules": "^0.12",
		"phpstan/phpstan-strict-rules": "^0.12",
		"thecodingmachine/phpstan-safe-rule": "^1",
		"roave/no-floaters": "^1.1.0",

		"symplify/easy-coding-standard": "^9.2",

		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Trejjam\\MailChimp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Trejjam\\MailChimp\\Tests\\": "tests/src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v0.5.x-dev"
		}
	}
}
