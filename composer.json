{
	"name": "trejjam/mail-chimp",
	"description": "MailChimp integration into Nette framework",
	"keywords": [
		"nette",
		"mailChimp"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal",
			"homepage": "https://github.com/trejjam"
		}
	],
	"require": {
		"php": "~8.1.0 | ~8.2.0",

		"composer/ca-bundle": "^1.3",
		"nette/di": "^3.1.2",
		"nette/utils": "^4.0",
		"guzzlehttp/guzzle": "^7.5",
		"tharos/schematic": "^1.5",
		"thecodingmachine/safe": "^2.5 | ^3.0.2"
	},
	"require-dev": {
		"nette/robot-loader": "^4.0",
		"nette/bootstrap": "^3.1",
		"tracy/tracy": "^2.9 || ^3.0",

		"nette/tester": "^2.5",

		"phpstan/phpstan": "^1.10",
		"phpstan/extension-installer": "^1.2",
		"phpstan/phpstan-deprecation-rules": "^1.1",
		"phpstan/phpstan-strict-rules": "^1.5",
		"nepada/phpstan-nette-tester": "^1.0",
		"thecodingmachine/phpstan-safe-rule": "^1.2",
		"roave/no-floaters": "^1.9",

		"symplify/easy-coding-standard": "^11.3 || ^12.0",

		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Trejjam\\MailChimp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Trejjam\\MailChimp\\Tests\\": "tests/src/",
			"Trejjam\\MailChimp\\Tests\\Cases\\": "tests/cases/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v0.7.x-dev"
		}
	}
}
